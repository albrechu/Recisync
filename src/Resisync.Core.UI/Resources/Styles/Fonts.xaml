<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="Resisync.Core.UI.Fonts"
    >
    <x:String x:Key="FontLight">SpaceGroteskLight</x:String>
    <x:String x:Key="FontRegular">SpaceGroteskRegular</x:String>
    <x:String x:Key="FontMedium">SpaceGroteskMedium</x:String>
    <x:String x:Key="FontBold">SpaceGroteskBold</x:String>
    <x:String x:Key="FontSemiBold">SpaceGroteskSemiBold</x:String>
    <x:String x:Key="FontIcons">MaterialIconsRegular</x:String>

    <x:Double x:Key="FontMicro">11</x:Double>
    <x:Double x:Key="FontSmall">12</x:Double>
    <x:Double x:Key="FontTitle">18</x:Double>
    <x:Double x:Key="FontSubtitle">16</x:Double>
</ResourceDictionary>